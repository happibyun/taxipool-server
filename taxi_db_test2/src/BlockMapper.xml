<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="BlockMapper">
 <insert id="insert" parameterType="Block">
	INSERT INTO block
    (info_id, 
    block_to_id)
	VALUES
    (#{info_id}, 
    #{block_to_id});
 </insert>
 </mapper>
  